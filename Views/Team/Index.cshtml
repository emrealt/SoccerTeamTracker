@model List<SoccerTeamTracker.Models.Team>
@{
}

<table class="table table-striped">
    <thead>
        <tr>
        <th>Name</th>
        <th>Color</th>
        </tr>

      </thead>
      <tbody>
          @foreach (var team in @Model)
          {
              <tr>
                  <td> @team.Name</td>
                  <td> @team.Color</td>
              </tr> 
          }

      </tbody>    
   </table>